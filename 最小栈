class MinStack:

    def __init__(self):
        """
        initialize your data structure here.
        """
        self.list = []

        
    def push(self, x):
        self.list.append(x)
        

    def pop(self):
        self.list.pop(-1)
        

    def top(self):
        return self.list[-1]
        

    def getMin(self):
        return min(self.list)

# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
