class Solution:
    def isValid(self, s) :
        """
        :type s: str
        """

        list1 = {'(': ')', '[': ']', '{': '}'}

        stack = [ ]

        for char in s:
            if char in list1:
                stack.append(char)
            else:
                if not stack or list1[stack.pop()] != char:
                    return False
        return not stack
        
